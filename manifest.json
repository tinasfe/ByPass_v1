{
  "manifest_version": 2,

  "name": "ByPass",
  "description": "Let's start storing your passwords",
  "version": "2.0.0",
  "icons": { "128": "bicon_128.png" },
  "browser_action": {
    "default_icon": "bicon_128.png",
    "default_popup": "popup.html",
    "optional_permissions": ["tabs"],
    "options_page": ["login.html"],
    "background_page":"create.html"

  },


  "permissions": [
    "activeTab",
    "tabs",
    "https://amazon.safaie.ca/",
    "https://tinapass.safaie.ca/",
    "http://webmail.montreal-events.com",
    "webNavigation",
    "storage"

  ]

}
